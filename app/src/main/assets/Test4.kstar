function void main() {
    float temperature = 99.5;

    if (temperature >= 100.0) {
        writeln("boiling point or above");
    } else if (temperature >= 37.0) {
        writeln("warm liquid");
    } else if (temperature >= 0.0) {
        writeln("cold liquid");
    } else {
        writeln("frozen or below");
    }
}
