//Addition of even numbers

function int main() {
    int sum = 0;
    int limit = 10;

    for (int i = 1; i <= limit; i = i + 1) {
        if (i % 2 == 0) {
            sum = sum + i;
        }
    }

    writeln("La suma de los nÃºmeros pares del 1 al 10 es:");
    writeln(sum);

    return 0;
}
